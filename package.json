{
  "name": "@selenic/webpack-plugin",
  "version": "0.7.0",
  "description": "webpack plugin for selenic",
  "keywords": [
    "license",
    "plugin",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mkwtys/selenic-webpack-plugin.git"
  },
  "license": "MIT",
  "author": "mkwtys <mkwtys@gmail.com>",
  "files": [
    "lib",
    "!lib/__tests__"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "coverage": "jest --coverage",
    "test": "jest",
    "test:update": "jest -u"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@selenic/core": "^0.9.0",
    "pkg-up": "^3.1.0",
    "webpack-sources": "^1.4.3"
  },
  "devDependencies": {
    "@babel/preset-env": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@types/jest": "24.9.0",
    "@types/memory-fs": "0.3.2",
    "@types/node": "12.12.25",
    "@types/pkg-up": "3.1.0",
    "@types/terser-webpack-plugin": "2.2.0",
    "@types/webpack": "4.39.9",
    "@types/webpack-sources": "0.1.6",
    "jest": "25.1.0",
    "memory-fs": "0.5.0",
    "terser-webpack-plugin": "2.3.2",
    "typescript": "3.7.5",
    "webpack": "4.41.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "changelog": {
    "repo": "mkwtys/selenic-webpack-plugin",
    "cacheDir": ".changelog",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":memo: Documentation",
      "dependencies": ":arrow_up: Dependencies",
      "internal": ":house: Internal"
    }
  }
}
